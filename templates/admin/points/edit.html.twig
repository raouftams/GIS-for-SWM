{% extends "base.html.twig" %}

{% block body %}

<form method="POST" action="">
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="inputCode">Code</label>
      <input type="text" class="form-control" id="inputCode" name="code">
    </div>
    <div class="form-group col-md-6">
      <label for="inputLibelle">Libelle</label>
      <input type="text" class="form-control" id="inputLibelle" name="libelle">
    </div>
  </div>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="inputLatitude">Latitude</label>
      <input type="number" step="0.000000000000001" name="latitude" class="form-control" id="inputLatitude">
    </div>
    <div class="form-group col-md-6">
      <label for="inputLongitude">Longitude</label>
      <input type="number" step="0.000000000000001" name="longitude" class="form-control" id="inputLongitude" >
    </div>
  </div>
  <div class="form-group">
    <label for="inputAddress">Address</label>
    <input type="text" class="form-control" name="adresse" id="inputAddress" placeholder="RN 05 COTE DOUZI 04">
  </div>
    <div class="form-group col-md-4">
      <label for="inputSecteur">Secteur</label>
      <select id="inputsecteur" name="secteur" class="form-control">
        <option selected>Choose...</option>
        <option>...</option>
      </select>
    </div>
  </div>
  <button type="submit" class="btn btn-primary">Sign in</button>
</form>

{% endblock %}